[alias]
    tree = log --abbrev-commit --all --decorate --graph --oneline

[color]
	ui = auto

[commit]
    gpgsign = true

[core]
	pager = diff-so-fancy | less -FRX

[credential]
	helper = cache

[filter "lfs"]
	clean = git-lfs clean -- %f
	required = true
	smudge = git-lfs smudge -- %f

[push]
	default = simple

